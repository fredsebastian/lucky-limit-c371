<style>
  .pub-list {
    display: flex;
    flex-direction: column;
    gap: 1.8em;
    margin: 2em 0;
  }

  .pub-entry {
    display: flex;
    gap: 1em;
    align-items: flex-start;
  }

  .pub-entry img {
    width: 100px;
    height: 100px;
    object-fit: cover;
    border-radius: 6px;
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);
    margin-top: 0.2em;
    flex-shrink: 0;
  }


  .pub-details {
    display: flex;
    flex-direction: column;
    justify-content: center;
    gap: 0.3em;
  }

  .title {
    font-size: 1.1rem;
    font-weight: 600;
    margin: 0;
    line-height: 1.3;
  }

  .authors {
    font-size: 0.85rem;
    color: #555;
    margin: 0;
  }

  .journal {
    font-size: 0.85rem;
    margin: 0;
    margin-top: -0.2em; /* brings journal line closer to authors */
  }

  .journal a {
    color: var(--accent, #0077b5);
    text-decoration: none;
  }

  .journal a:hover {
    text-decoration: underline;
  }

  .pdf-link {
    margin-left: 0.4em;
    display: inline-flex;
    align-items: center;
    gap: 0.2em;
    font-size: 0.75rem;
    color: #666; /* neutral tone */
    font-weight: 400;
    text-decoration: none;
  }

  .pdf-link:hover {
   color: #0077b5; /* your site’s accent color */
  }

  .pdf-icon {
    width: 14px;
    height: 14px;
    vertical-align: middle;
  }

  .pdf-text {
    font-size: 0.8rem;
    font-weight: 400;
    line-height: 1;
  }

  .pub-section {
    font-size: 1.5rem;
    font-weight: 700;
    margin: 3em 0 1.5em;
    padding-bottom: 0.3em;
    border-bottom: 2px solid #ddd;
    font-family: 'Georgia', 'Times New Roman', serif;
    color: #444;
    letter-spacing: 0.4px;
    text-transform: uppercase;
  }



  @media (max-width: 700px) {
    .pub-entry {
      flex-direction: column;
      align-items: center;
      text-align: center;
      gap: 1em;
      padding: 1em 0;
    }

    .pub-details {
      align-items: center;
      gap: 0.4em;
    }

    .title {
      font-size: 1rem;
      font-weight: 600;
      text-align: center;
    }

    .authors, .journal {
      font-size: 0.8rem;
    }

    .pdf-text {
      font-size: 0.75rem;
    }

    .pub-entry img {
      width: 125px;
      height: 125px;
    }
  }

</style>
